{
  "name": "victor-hugo",
  "version": "1.0.0",
  "description": "Victor Hugo is a Hugo boilerplate for creating truly epic websites!",
  "repository": "netlify/victor-hugo",
  "main": "index.js",
  "scripts": {
    "lint": "eslint src",
    "start": "run-p start:**",
    "start:hugo": "hugo -d ../dist -s site -vw",
    "start:webpack": "webpack serve --config webpack.dev.js --hot",
    "preview": "run-p preview:**",
    "preview:hugo": "npm run start:hugo -- -D -F",
    "preview:webpack": "npm run start:webpack",
    "prebuild": "rimraf dist",
    "build": "npm run build:webpack && npm run build:hugo",
    "build:preview": "npm run build:webpack && npm run build:hugo:preview",
    "build:hugo": "hugo version && hugo -d ../dist -s site -v",
    "build:hugo:preview": "npm run build:hugo -- -D -F",
    "build:webpack": "cross-env NODE_ENV=production webpack --config webpack.prod.js",
    "buildng": "npm run buildng:webpack && exec-bin node_modules/.bin/hugo/hugo -d ../dist -s site -v",
    "buildng:preview": "npm run buildng:webpack && npm run buildng:hugo:preview",
    "buildng:hugo": "exec-bin node_modules/.bin/hugo/hugo -d ../dist -s site -v",
    "buildng:hugo:preview": "npm run buildng:hugo -- -D -F",
    "buildng:webpack": "cross-env NODE_ENV=production webpack --config webpack.prod.js",
    "postinstall": "hugo-installer --version otherDependencies.hugo --extended --destination node_modules/.bin/hugo"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.17.2",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.16.7",
    "@babel/plugin-syntax-object-rest-spread": "^7.8.3",
    "@babel/preset-env": "^7.16.11",
    "@babel/register": "^7.17.0",
    "@webpack-cli/serve": "^1.6.1",
    "assets-webpack-plugin": "^7.1.1",
    "babel-loader": "^9.0.0",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^12.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^7.0.0",
    "css-minimizer-webpack-plugin": "^7.0.0",
    "eslint": "^8.9.0",
    "eslint-plugin-import": "^2.25.4",
    "exports-loader": "^5.0.0",
    "fancy-log": "^2.0.0",
    "imports-loader": "^5.0.0",
    "mini-css-extract-plugin": "^2.5.3",
    "npm-run-all2": "^7.0.0",
    "plugin-error": "^2.0.0",
    "postcss-import": "^16.0.0",
    "postcss-loader": "^8.0.0",
    "postcss-preset-env": "^10.0.0",
    "rimraf": "^6.0.1",
    "sass": "^1.53.0",
    "sass-loader": "^15.0.0",
    "style-loader": "^4.0.0",
    "terser-webpack-plugin": "^5.3.1",
    "url-loader": "^4.1.1",
    "webpack": "^5.69.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^5.0.0",
    "webpack-merge": "^6.0.0",
    "whatwg-fetch": "^3.6.2",
    "lint-staged": "15.2.10",
    "markdownlint-cli": "0.42.0",
    "autoprefixer": "^10.4",
    "postcss": "^8.4",
    "postcss-cli": "^11.0",
    "exec-bin": "^1.0.0",
    "hugo-installer": "^4.0"
  },
  "resolutions": {
    "natives": "1.1.6"
  },
  "dependencies": {
    "bootstrap": "^4.5.2",
    "flickity": "^3.0.0",
    "flickity-fade": "^1.0.0",
    "jquery": "^3.5.1",
    "lazysizes": "^5.3.2",
    "popper.js": "^1.16.1",
    "scroll-out": "^2.2.12",
    "sticky-js": "^1.3.0"
  },
  "otherDependencies": {
    "hugo": "0.136.2"
  },
  "lint-staged": {
    "*.md": "markdownlint --config .markdownlint.json --fix"
  }
}
