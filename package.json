{
  "name": "victor-hugo",
  "version": "1.0.0",
  "description": "Victor Hugo is a Hugo boilerplate for creating truly epic websites!",
  "repository": "netlify/victor-hugo",
  "main": "index.js",
  "scripts": {
    "lint": "eslint src",
    "start": "run-p start:**",
    "start:hugo": "hugo -d ../dist -s site -vw",
    "start:webpack": "webpack serve --config webpack.dev.js --hot",
    "preview": "run-p preview:**",
    "preview:hugo": "npm run start:hugo -- -D -F",
    "preview:webpack": "npm run start:webpack",
    "prebuild": "rimraf dist",
    "build": "npm run build:webpack && npm run build:hugo",
    "build:preview": "npm run build:webpack && npm run build:hugo:preview",
    "build:hugo": "hugo version && hugo -d ../dist -s site -v",
    "build:hugo:preview": "npm run build:hugo -- -D -F",
    "build:webpack": "cross-env NODE_ENV=production webpack --config webpack.prod.js"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.13.16",
    "@babel/eslint-parser": "^7.13.14",
    "@babel/plugin-proposal-object-rest-spread": "^7.13.8",
    "@babel/plugin-syntax-object-rest-spread": "^7.8.3",
    "@babel/preset-env": "^7.13.15",
    "@babel/register": "^7.13.16",
    "@webpack-cli/serve": "^1.4.0",
    "assets-webpack-plugin": "^7.0.0",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "copy-webpack-plugin": "^8.1.1",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.3",
    "css-minimizer-webpack-plugin": "^2.0.0",
    "eslint": "^7.24.0",
    "eslint-plugin-import": "^2.22.1",
    "exports-loader": "^2.0.0",
    "fancy-log": "^1.3.3",
    "file-loader": "^6.2.0",
    "imports-loader": "^2.0.0",
    "mini-css-extract-plugin": "^1.5.0",
    "node-sass": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "plugin-error": "^1.0.1",
    "postcss-import": "^14.0.1",
    "postcss-loader": "^5.2.0",
    "postcss-preset-env": "^6.7.0",
    "rimraf": "^3.0.2",
    "sass-loader": "^11.0.1",
    "style-loader": "^2.0.0",
    "terser-webpack-plugin": "^5.1.1",
    "url-loader": "^4.1.1",
    "webpack": "^5.34.0",
    "webpack-cli": "^4.6.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3",
    "whatwg-fetch": "^3.6.2"
  },
  "resolutions": {
    "natives": "1.1.6"
  },
  "dependencies": {
    "bootstrap": "^4.5.2",
    "flickity": "^2.2.1",
    "flickity-fade": "^1.0.0",
    "jquery": "^3.5.1",
    "popper.js": "^1.16.1",
    "scroll-out": "^2.2.12",
    "sticky-js": "^1.3.0"
  }
}
